.wrapper.boxed-wrapper
  header.main-header
    // Logo
    a.logo.blue-bg(href='index.html')
      // mini logo for sidebar mini 50x50 pixels
      span.logo-mini
        img(src='dist/img/logo.png' alt='' width='25%')
      // logo for regular state and mobile devices
      span.logo-lg
        img(src='dist/img/logo.png' alt='' width='25%')
    // Header Navbar: style can be found in header.less
    nav.navbar.blue-bg.navbar-static-top
      // Sidebar toggle button
      ul.nav.navbar-nav.pull-left
        li
          a.sidebar-toggle(data-toggle='push-menu' href='')
      .pull-left.search-box
        form.search-form(action='#' method='get')
          .input-group
            input.form-control(name='search' placeholder='Search...' type='text')
            span.input-group-btn
              button#search-btn.btn.btn-flat(type='submit' name='search')
                i.fa.fa-search
        // search form
      .navbar-custom-menu
  // Left side column. contains the logo and sidebar
  aside.main-sidebar
    // sidebar: style can be found in sidebar.less
    .sidebar
      // Sidebar user panel
      ul.sidebar-menu(data-widget='tree')
        li.active.treeview
          a(href='#')
            i.fa.fa-dashboard
            span Dashboard
            span.pull-right-container
              i.fa.fa-angle-left.pull-right
          ul.treeview-menu
            li.active
              a(href='/') Intro
            li
              a(href='/dash_overview') ETHO supply distribution
            li
              a(href='/dash_financial') ETHO financial

            li
              a(href='/dash_financial2') ETHO If i had ...
            li
              a(href='/dash_cmctrending') ETHO trending
            li
              a(href='/dash_nodes') ETHO nodes
//            li
              a(href='/dash_health') ETHO asset health
            li
              a(href='/dash_exchanges') ETHO exchanges
            li
              a(href='/dash_richlist') ETHO richlist
            li
              a(href='/dash_ipfs') ETHO IPFS
            li
              a(href='/dash_wetho') ETHO wrapped

//            li
              a(href='/dash_activity') ETHO activity
//            li
              a(href='/dash_subscribe') Subscribe to events








    // /.sidebar



// Content Wrapper. Contains page content
.content-wrapper
  // HUH
  != messages('message', locals)
  if errors
    each error, i in errors
      div(class="alert alert-danger") #{error.msg}


  block content

  // /.content-wrapper
  footer.main-footer
    if !data
      .pull-right.hidden-xs Version 1.10
    else
      .pull-right.hidden-xs= data.date
